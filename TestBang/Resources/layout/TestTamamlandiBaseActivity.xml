<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#1EB04B"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img2"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#11122E"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout 
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <android.support.v7.widget.CardView
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cardView1" >
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout1" >
                   <TextView
                       android:textStyle="bold"
                       android:gravity="center"
                       android:textColor="#1EB04B"
                       android:text="TEST TAMAMLANDI"
                       android:textAppearance="?android:attr/textAppearanceMedium"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:id="@+id/textView1" />
                      <ImageView
                          android:src="@mipmap/sifremiunuttum_img2"
                          android:layout_width="match_parent"
                          android:layout_height="124.5dp"
                          android:id="@+id/ımageView2"
                          android:layout_marginBottom="12.0dp"
                          android:layout_marginTop="7.0dp" />
                      <TextView
                           android:layout_marginBottom="5dp"
                           android:textStyle="bold"
                           android:gravity="center"
                           android:textColor="#11122E"
                           android:text="Toplam Süre"
                           android:textAppearance="?android:attr/textAppearanceMedium"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:id="@+id/textView1" />
                     <TextView
                           android:layout_marginBottom="5dp"
                           android:textStyle="bold"
                           android:gravity="center"
                           android:textColor="#11122E"
                           android:text="00:85:00"
                           android:textAppearance="?android:attr/textAppearanceMedium"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:id="@+id/toplamsuretext" />
                </LinearLayout>
            </android.support.v7.widget.CardView>
                <LinearLayout
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="15dp"
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout7">
                <TextView
                    android:textSize="12dp"
                    android:gravity="center_horizontal"
                    android:textColor="#11122E"
                    android:text="GENEL TEST SONUÇLARI"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:layout_marginBottom="4.0dp" />
                <LinearLayout
                    android:weightSum="99"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout8">
                    <LinearLayout
                        android:layout_weight="33"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout9">
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#1EB04B"
                            android:text="Doğru Cevap"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView6" />
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#1EB04B"
                            android:text="%39"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/dogrucevapyuzde" />
                          <RelativeLayout
		                    android:layout_marginTop="5dp"
		                    android:minWidth="25px"
		                    android:minHeight="25px"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:id="@+id/relativeLayout1">
		                    <ProgressBar
			                    android:layout_centerHorizontal="true"
			                    android:id="@+id/dogruprogress"
			                    android:layout_width="80dp"
			                    android:layout_height="80dp"
			                    android:indeterminate="false"
			                    android:progressDrawable="@drawable/progres_dogru"
			                    android:background="@drawable/circle_shape"
			                    style="?android:attr/progressBarStyleHorizontal"
			                    android:max="100"
			                    android:progress="50" />
		                    <TextView
			                    android:textStyle="bold"
			                    android:textSize="10dp"
			                    android:textColor="#1EB04B"
			                    android:layout_centerInParent="true"
			                    android:text="1.249"
			                    android:textAppearance="?android:attr/textAppearanceLarge"
			                    android:layout_width="wrap_content"
			                    android:layout_height="wrap_content"
			                    android:id="@+id/dogrucevapsayi" />
	                    </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="33"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout9">
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#F40621"
                            android:text="Yanlış Cevap"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView6" />
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#F40621"
                            android:text="%39"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/yalniscevapyuzde" />
                          <RelativeLayout
		                    android:layout_marginTop="5dp"
		                    android:minWidth="25px"
		                    android:minHeight="25px"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:id="@+id/relativeLayout1">
		                    <ProgressBar
			                    android:layout_centerHorizontal="true"
			                    android:id="@+id/yanlisprogress"
			                    android:layout_width="80dp"
			                    android:layout_height="80dp"
			                    android:indeterminate="false"
			                    android:progressDrawable="@drawable/progres_yanlis"
			                    android:background="@drawable/circle_shape"
			                    style="?android:attr/progressBarStyleHorizontal"
			                    android:max="100"
			                    android:progress="70" />
		                    <TextView
			                    android:textStyle="bold"
			                    android:textSize="10dp"
			                    android:textColor="#F40621"
			                    android:layout_centerInParent="true"
			                    android:text="1.249"
			                    android:textAppearance="?android:attr/textAppearanceLarge"
			                    android:layout_width="wrap_content"
			                    android:layout_height="wrap_content"
			                    android:id="@+id/yalniscevapsayi" />
	                    </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="33"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout9">
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#EF506F"
                            android:text="Cevapsız"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView6" />
                        <TextView
                            android:textSize="10dp"
                            android:gravity="center_horizontal"
                            android:textColor="#EF506F"
                            android:text="%39"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/boscevapyuzde" />
                          <RelativeLayout
		                    android:layout_marginTop="5dp"
		                    android:minWidth="25px"
		                    android:minHeight="25px"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:id="@+id/relativeLayout1">
		                    <ProgressBar
			                    android:layout_centerHorizontal="true"
			                    android:id="@+id/bosprogress"
			                    android:layout_width="80dp"
			                    android:layout_height="80dp"
			                    android:indeterminate="false"
			                    android:progressDrawable="@drawable/progres_cevapsiz"
			                    android:background="@drawable/circle_shape"
			                    style="?android:attr/progressBarStyleHorizontal"
			                    android:max="100"
			                    android:progress="30" />
		                    <TextView
			                    android:textStyle="bold"
			                    android:textSize="10dp"
			                    android:textColor="#EF506F"
			                    android:layout_centerInParent="true"
			                    android:text="1.249"
			                    android:textAppearance="?android:attr/textAppearanceLarge"
			                    android:layout_width="wrap_content"
			                    android:layout_height="wrap_content"
			                    android:id="@+id/boscevapsayi" />
	                    </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="15dp"
                 android:layout_gravity="center_horizontal"
                 android:layout_marginTop="50dp"
                 android:orientation="horizontal"
                 android:minWidth="25px"
                 android:minHeight="25px"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:id="@+id/linearLayout15" >
                 <Button
                     android:textSize="10dp"
                     android:textColor="#fff"
                     android:background="@drawable/profiliguncellebuttonbg"
                     android:text="YENİ BİR TESTE BAŞLA"
                     android:layout_width="match_parent"
                     android:layout_height="45dp"
                     android:id="@+id/button2" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
