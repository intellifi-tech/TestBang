<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F05070"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img2"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="100dp" />
        <androidx.core.widget.NestedScrollView
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/nestedScrollView1">
            <RelativeLayout
                android:orientation="vertical"
                android:background="@android:color/transparent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <androidx.cardview.widget.CardView
                    android:visibility="visible"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="20dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="50dp"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cardView1">
                    <LinearLayout
                        android:layout_marginTop="40dp"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout1">
                        <TextView
                            android:layout_marginTop="15dp"
                            android:textColor="#1EB14C"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"
                            android:text="TRANSKRIPT"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2" />
                        <TextView
                            android:layout_marginTop="5dp"
                            android:textColor="#11122E"
                            android:gravity="center_horizontal"
                            android:text=".."
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@id/adsoyadtext" />
                        <TextView
                            android:layout_marginRight="20dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginBottom="20dp"
                            android:textSize="12dp"
                            android:layout_marginTop="5dp"
                            android:textColor="#11122E"
                            android:gravity="center_horizontal"
                            android:text="Deneme sınavı performanslarınızın genel değerlendirmesini aşağıda bulabilirsiniz."
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView3" />
                        <FrameLayout
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/frameLayout1" />
                        
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_marginTop="20dp"
                    android:layout_below="@+id/cardView1"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout10" >
                    <LinearLayout
                            android:layout_gravity="center_horizontal"
                            android:weightSum="100"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout3">
                            <LinearLayout
                                android:layout_gravity="center_vertical"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_weight="30"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout4">
                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/linearLayout5">
                                    <TextView
                                        android:textSize="12dp"
                                        android:textStyle="bold"
                                        android:textColor="#11122E"
                                        android:text="transkript no:"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:textSize="12dp"
                                        android:maxLines="1"
                                        android:layout_marginLeft="5dp"
                                        android:text="000"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView5" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginLeft="50dp"
                                android:layout_weight="70"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:weightSum="99"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout6">
                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/linearLayout5">
                                    <TextView
                                        android:textSize="12dp"
                                        android:textStyle="bold"
                                        android:textColor="#11122E"
                                        android:text="tarih:"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:maxLines="1"
                                        android:textSize="12dp"
                                        android:layout_marginLeft="5dp"
                                        android:text="MART/29"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView5" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    <LinearLayout
                            android:layout_marginRight="30dp"
                            android:layout_marginLeft="30dp"
                            android:layout_marginTop="20dp"
                            android:weightSum="99"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout8">
                            <LinearLayout
                                android:layout_weight="33"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:id="@+id/linearLayout9">
                                <TextView
                                    android:textSize="10dp"
                                    android:gravity="center_horizontal"
                                    android:textColor="#1EB04B"
                                    android:text="Doğru"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView6" />
                                <RelativeLayout
                                    android:layout_marginTop="5dp"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout1">
                                    <ProgressBar
                                        android:layout_centerHorizontal="true"
                                        android:id="@+id/dogruprogress"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        android:indeterminate="false"
                                        android:progressDrawable="@drawable/progres_dogru"
                                        android:background="@drawable/circle_shape"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:max="100"
                                        android:progress="50" />
                                    <TextView
                                        android:textStyle="bold"
                                        android:textSize="10dp"
                                        android:textColor="#1EB04B"
                                        android:layout_centerInParent="true"
                                        android:text="1.249"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/dogrucevapsayi" />
                                </RelativeLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_weight="33"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:id="@+id/linearLayout9">
                                <TextView
                                    android:textSize="10dp"
                                    android:gravity="center_horizontal"
                                    android:textColor="#F40621"
                                    android:text="Yanlış"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView6" />
                                <RelativeLayout
                                    android:layout_marginTop="5dp"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout1">
                                    <ProgressBar
                                        android:layout_centerHorizontal="true"
                                        android:id="@+id/yanlisprogress"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        android:indeterminate="false"
                                        android:progressDrawable="@drawable/progres_yanlis"
                                        android:background="@drawable/circle_shape"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:max="100"
                                        android:progress="70" />
                                    <TextView
                                        android:textStyle="bold"
                                        android:textSize="10dp"
                                        android:textColor="#F40621"
                                        android:layout_centerInParent="true"
                                        android:text="1.249"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/yalniscevapsayi" />
                                </RelativeLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_weight="33"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:id="@+id/linearLayout9">
                                <TextView
                                    android:textSize="10dp"
                                    android:gravity="center_horizontal"
                                    android:textColor="#EF506F"
                                    android:text="Cevapsız"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView6" />
                                <RelativeLayout
                                    android:layout_marginTop="5dp"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout1">
                                    <ProgressBar
                                        android:layout_centerHorizontal="true"
                                        android:id="@+id/bosprogress"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        android:indeterminate="false"
                                        android:progressDrawable="@drawable/progres_cevapsiz"
                                        android:background="@drawable/circle_shape"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:max="100"
                                        android:progress="30" />
                                    <TextView
                                        android:textStyle="bold"
                                        android:textSize="10dp"
                                        android:textColor="#EF506F"
                                        android:layout_centerInParent="true"
                                        android:text="1.249"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/boscevapsayi" />
                                </RelativeLayout>
                            </LinearLayout>
                        </LinearLayout>
                  </LinearLayout>
                <RelativeLayout
                    android:elevation="20dp"
                    android:layout_centerHorizontal="true"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:id="@+id/relativeLayout5">
                    <ImageView
                        android:layout_centerInParent="true"
                        android:src="@mipmap/transkriptsiralamaimg"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:id="@+id/ımageView2" />
                    <LinearLayout
                        android:weightSum="100"
                        android:layout_centerInParent="true"
                        android:background="@drawable/transkriptsiramabg"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:id="@+id/linearLayout2">
                        <TextView
                            android:textSize="30dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="17."
                            android:textStyle="bold"
                            android:textColor="#fff"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="60"
                            android:id="@+id/textView4" />
                        <TextView
                            android:textSize="10dp"
                            android:gravity="top|center_horizontal"
                            android:text="SIRADASIN"
                            android:textStyle="bold"
                            android:textColor="#fff"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="40"
                            android:id="@+id/textView4" />
                    </LinearLayout>
                </RelativeLayout>
                <LinearLayout
                    android:layout_marginTop="25dp"
                    android:layout_below="@+id/linearLayout10"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout11" >
                    <TextView
                        android:gravity="center_horizontal"
                        android:textStyle="bold"
                        android:text="KONULAR"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView7" />
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:background="#141436"
                        android:orientation="horizontal"
                        android:layout_width="100dp"
                        android:layout_height="2dp"
                        android:id="@+id/linearLayout12" />
                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="15dp"
                        android:nestedScrollingEnabled="false"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/recyclerView1" />
                    <Button
                        android:layout_marginRight="30dp"
                        android:layout_marginLeft="30dp"
                         android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp"
                        android:textSize="10dp"
                        android:textColor="#fff"
                        android:background="@drawable/girisbuttonbg"
                        android:text="TRANSKRIPT PAYLAŞ"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:id="@+id/button3" />
                </LinearLayout>
            </RelativeLayout>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>
</LinearLayout>
