<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#1eb14c"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout16">
        <RelativeLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="65.5dp"
            android:id="@+id/relativeLayout1"
            android:layout_marginTop="17.5dp">
            <ImageView
                android:scaleType="centerInside"
                android:src="@mipmap/testbang_logo_img2"
                android:layout_width="163.5dp"
                android:layout_height="wrap_content"
                android:id="@+id/ımageView1"
                android:layout_marginLeft="25.0dp" />
            <LinearLayout
                android:clickable="true"
                android:layout_alignParentRight="true"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/bildirimhaznebutton"
                android:layout_marginRight="25dp">
                <ImageButton
                    android:padding="8dp"
                    android:scaleType="centerInside"
                    android:tint="#ffffff"
                    android:background="?android:attr/selectableItemBackground"
                    android:src="@drawable/pencil"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageButton1" />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout3"
            android:layout_marginTop="0.0dp">
            <RelativeLayout
                android:background="#fff"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/relativeLayout4"
                android:layout_marginTop="60dp" />
            <androidx.viewpager.widget.ViewPager
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@+id/linearLayout14"
                android:id="@+id/viewPager1" />
            <LinearLayout
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="15dp"
                android:layout_alignParentBottom="true"
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout14">
                <LinearLayout
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/girisbuttonbg"
                    android:layout_alignParentBottom="true"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:weightSum="99"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout14">
                    <Button
                        android:fontFamily="@font/monserratfontbold"
                        android:textSize="10dp"
                        android:textColor="#fff"
                        android:background="@android:color/transparent"
                        android:text="ÖNCEKİ SORU"
                        android:layout_width="0dp"
                        android:layout_weight="33"
                        android:layout_height="match_parent"
                        android:id="@+id/oncekisorubutton" />
                    <LinearLayout
                        android:background="#11122E"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:weightSum="100"
                        android:minHeight="25px"
                        android:layout_width="0dp"
                        android:layout_weight="33"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout15">
                        <TextView
                            android:fontFamily="@font/monserratfontbold"
                            android:paddingBottom="3dp"
                            android:gravity="center_horizontal|bottom"
                            android:textColor="#fff"
                            android:textSize="10dp"
                            android:text="KALAN SÜRE"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="50"
                            android:id="@+id/textView9" />
                        <TextView
                            
                            android:textSize="15dp"
                            android:gravity="center_horizontal|top"
                            android:textColor="#fff"
                            android:textStyle="bold"
                            android:text="00:00:00"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="50"
                            android:id="@+id/suretext" />
                    </LinearLayout>
                    <Button
                        android:fontFamily="@font/monserratfontbold"
                        android:textSize="10dp"
                        android:textColor="#fff"
                        android:background="@android:color/transparent"
                        android:text="SONRAKİ SORU"
                        android:layout_width="0dp"
                        android:layout_weight="33"
                        android:layout_height="match_parent"
                        android:id="@+id/sonrakisorubutton" />
                </LinearLayout>
                <Button
                    android:fontFamily="@font/monserratfontbold"
                    android:background="@drawable/gributtonbg"
                    android:text="TESTİ DURDUR"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/araverbutton" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/cizimhazne" />
</RelativeLayout>
