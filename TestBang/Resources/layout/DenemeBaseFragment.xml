
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.widget.NestedScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <android.support.v7.widget.CardView
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="231.0dp"
                android:id="@+id/cardView1">
                <RelativeLayout
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout1">
                    <android.support.v4.view.ViewPager
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/tabLayoutchart"
                        android:id="@+id/viewPager1chart" />
                    <android.support.design.widget.TabLayout
                        android:layout_alignParentTop="true"
                        android:id="@+id/tabLayoutchart"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#fff"
                        android:minHeight="?attr/actionBarSize"
                        app:tabIndicatorColor="#F05070"
                        app:tabIndicatorHeight="2dp"
                        app:tabSelectedTextColor="#F05070"
                        app:tabMode="scrollable"
                        app:tabGravity="fill"
                        app:tabTextColor="#909090"
                        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
             <TextView
                    android:layout_marginTop="15dp"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:textColor="#F05070"
                    android:text="MERVE TUĞAL"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView1" />
             <TextView
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    android:textSize="11dp"
                    android:textColor="#11122e"
                    android:text="TURNASUYU, ORDU"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView1" />
            <LinearLayout
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:weightSum="100"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout12">
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_weight="40"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout13">
                    <android.support.v7.widget.CardView
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="5dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/cardView1" >
                        <LinearLayout
                            android:paddingTop="10dp"
                            android:paddingBottom="5dp"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout16" >
                            <TextView
                                android:gravity="center"
			                    android:textStyle="bold"
			                    android:textSize="12dp"
			                    android:textColor="#11122E"
			                    android:layout_centerInParent="true"
			                    android:text="Ulusal Deneme Sınavı"
			                    android:textAppearance="?android:attr/textAppearanceLarge"
			                    android:layout_width="match_parent"
			                    android:layout_height="wrap_content"
			                    android:id="@+id/textView3" />
                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:weightSum="100"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout14" >
                                <LinearLayout
                                   android:orientation="vertical"
                                   android:minWidth="25px"
                                   android:minHeight="25px"
                                   android:layout_weight="50"
                                   android:layout_width="0dp"
                                   android:layout_height="match_parent"
                                   android:id="@+id/linearLayout15" >
                                   <TextView
                                        android:gravity="center"
			                            android:textStyle="bold"
			                            android:textSize="10dp"
			                            android:textColor="#11122E"
			                            android:layout_centerInParent="true"
			                            android:text="Mevcut Sıralama"
			                            android:textAppearance="?android:attr/textAppearanceLarge"
			                            android:layout_width="match_parent"
			                            android:layout_height="wrap_content"
			                            android:id="@+id/textView3" />
                                    <TextView
                                        android:paddingLeft="5dp"
                                        android:paddingRight="5dp"
                                        android:paddingTop="5dp"
                                        android:textColor="#11122E"
                                        android:gravity="center"
                                        android:textSize="18dp"
                                        android:textStyle="bold"
                                        android:text="1.246"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/textView2" />
                                </LinearLayout>
                                <View
                                    android:background="#E1E1E1"
                                    android:layout_width="0.5dp"
                                    android:layout_height="60dp"
                                    android:id="@+id/view1" />
                                <LinearLayout
                                   android:orientation="vertical"
                                   android:minWidth="25px"
                                   android:minHeight="25px"
                                   android:layout_weight="50"
                                   android:layout_width="0dp"
                                   android:layout_height="match_parent"
                                   android:id="@+id/linearLayout15" >
                                    <TextView
                                        android:gravity="center"
			                            android:textStyle="bold"
			                            android:textSize="10dp"
			                            android:textColor="#11122E"
			                            android:layout_centerInParent="true"
			                            android:text="En Yüksek Sıralaması"
			                            android:textAppearance="?android:attr/textAppearanceLarge"
			                            android:layout_width="match_parent"
			                            android:layout_height="wrap_content"
			                            android:id="@+id/textView3" />
                                    <TextView
                                        android:paddingLeft="5dp"
                                        android:paddingRight="5dp"
                                        android:paddingTop="5dp"
                                        android:textColor="#11122E"
                                        android:gravity="center"
                                        android:textSize="18dp"
                                        android:textStyle="bold"
                                        android:text="1.246"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/textView2" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                </LinearLayout>
                <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_weight="60"
                    android:layout_width="0dp"
                    android:layout_height="238.0dp"
                    android:id="@+id/relativeLayout1">
                    <android.support.v7.widget.RecyclerView
                        android:nestedScrollingEnabled="true"
                        android:layout_marginTop="0.0dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/recyclerView1" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="15dp"
                android:layout_alignParentBottom="true"
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout14" >
            <LinearLayout
                android:layout_marginBottom="15dp"
                android:background="@drawable/girisbuttonbg"
                android:layout_alignParentBottom="true"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:weightSum="100"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/buttonsHazne" >
            <Button
                android:textColor="#fff"
                android:background="#131433"
                android:text="GEÇMİŞ DENEME SINAVI"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:layout_height="match_parent"
                android:id="@+id/oncekisorubutton" />
            <Button
                android:textColor="#fff"
                android:background="#ED4F6F"
                android:text="GELECEK DENEME SINAVI"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:layout_height="match_parent"
                android:id="@+id/sonrakisorubutton" />
            </LinearLayout>
            <Button
                android:textColor="#fff"
                android:background="@drawable/girisbuttonbg"
                android:text="DENEME SINAVINA KATIL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/araverbutton" />
         </LinearLayout>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>