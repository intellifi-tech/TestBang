                        app:cardCornerRadius="20dp"
                        android:layout_marginRight="30dp"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/cardView1">
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginRight="10dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout1">
                            <RelativeLayout
                                android:paddingTop="10dp"
                                android:paddingLeft="10dp"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/relativeLayout1">
                                <RelativeLayout
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout2">
                                    <LinearLayout
                                        android:layout_toLeftOf="@+id/relativeLayout3"
                                        android:orientation="vertical"
                                        android:minWidth="25px"
                                        android:minHeight="25px"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/linearLayout2">
                                        <TextView
                                            android:fontFamily="@font/monserratfont"
                                            android:textColor="#11122e"
                                            android:text="Selam,"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/textView1" />
                                        <TextView
                                            android:fontFamily="@font/monserratfontbold"
                                            android:textSize="20dp"
                                            android:textStyle="bold"
                                            android:textColor="#F05070"
                                            android:text="MERVE TUĞAL"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/adsoyadtext" />
                                    </LinearLayout>
                                    <RelativeLayout
                                        android:layout_alignParentRight="true"
                                        android:minWidth="25px"
                                        android:minHeight="25px"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        android:id="@+id/relativeLayout3">
