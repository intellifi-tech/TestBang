                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textPersonName"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/editText1" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/hesapolusturinputbg"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_weight="50"
                                android:layout_height="50.0dp"
                                android:id="@+id/linearLayout1">
                                <ImageView
                                    android:tint="#5011122E"
                                    android:padding="5dp"
                                    android:src="@mipmap/login_email_icon"
                                    android:layout_width="30.5dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/ımageView2"
                                    android:layout_marginLeft="15.0dp" />
                                <EditText
                                    android:fontFamily="@font/monserratfont"
                                    android:textCursorDrawable="@null"
                                    android:textSize="16dp"
                                    android:paddingLeft="15dp"
                                    android:background="@null"
                                    android:textColor="#11122E"
                                    android:hint="Soyadınız"
                                    android:textColorHint="#9011122E"
                                    android:layout_marginLeft="0.0dp"
                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textPersonName"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/editText2" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
