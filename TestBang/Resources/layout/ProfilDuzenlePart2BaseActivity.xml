<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F05070"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img2"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
        <LinearLayout
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout1"
            android:layout_marginRight="25dp">
            <RelativeLayout
                android:layout_gravity="center_vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/relativeLayout2">
                <ImageView
                    android:layout_marginTop="10dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/notification1"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:id="@+id/ımageView2" />
                <TextView
                    android:textSize="13dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:background="@drawable/bildirimIcinBadge"
                    android:textColor="#F05070"
                    android:text="3"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:id="@+id/textView1" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout
            android:orientation="vertical"
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ScrollView
                android:scrollbars="none"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/scrollView1">
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout11">
                    <android.support.v7.widget.CardView
                        app:cardElevation="10dp"
                        app:cardCornerRadius="20dp"
                        android:layout_marginRight="30dp"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/cardView1" >
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginRight="10dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout1" >
                            <RelativeLayout
                                android:paddingTop="10dp"
                                android:paddingLeft="10dp"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/relativeLayout1">
                                <RelativeLayout
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout2">
                                    <LinearLayout
                                        android:layout_toLeftOf="@+id/relativeLayout3"
                                        android:orientation="vertical"
                                        android:minWidth="25px"
                                        android:minHeight="25px"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/linearLayout2">
                                        <TextView
                                            android:textColor="#11122e"
                                            android:text="Selam,"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/textView1" />
                                        <TextView
                                            android:textSize="20dp"
                                            android:textStyle="bold"
                                            android:textColor="#F05070"
                                            android:text="MERVE TUĞAL"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/textView1" />
                                    </LinearLayout>
                                    <RelativeLayout
                                        android:layout_alignParentRight="true"
                                        android:minWidth="25px"
                                        android:minHeight="25px"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        android:id="@+id/relativeLayout3" >
                                        <refractored.controls.CircleImageView
                                            android:background="@mipmap/profilebgimg1"
                                              android:layout_centerInParent="true"
                                              android:id="@+id/profile_image"
                                              android:layout_width="match_parent"
                                              android:layout_height="match_parent"
                                              android:src="@mipmap/ornekfoto"
                                              app:civ_border_width="16dp"
                                              app:civ_border_color="@android:color/transparent"/>
                                    </RelativeLayout>
                                </RelativeLayout>
                            </RelativeLayout>
                    
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout16">
               <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:weightSum="100"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout4"
                android:layout_marginTop="15.5dp">
                <LinearLayout
                    android:layout_marginRight="10dp"
                    android:background="@drawable/hesapolusturinputbg"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="0dp"
                    android:layout_weight="50"
                    android:layout_height="50.0dp"
                    android:id="@+id/linearLayout1">
                    <ImageView
                        android:tint="#5011122E"
                        android:padding="5dp"
                        android:src="@mipmap/login_email_icon"
                        android:layout_width="30.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/ımageView2"
                        android:layout_marginLeft="15.0dp" />
                    <EditText
                        android:textSize="12dp"
                        android:paddingLeft="15dp"
                        android:background="@null"
                        android:textColor="#11122E"
                        android:hint="Adınız"
                        android:textColorHint="#9011122E"
                        android:layout_marginLeft="0.0dp"
                        android:layout_marginRight="0.0dp"
                        android:inputType="textEmailAddress"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/editText1" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/hesapolusturinputbg"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="0dp"
                    android:layout_weight="50"
                    android:layout_height="50.0dp"
                    android:id="@+id/linearLayout1">
                    <ImageView
                        android:tint="#5011122E"
                        android:padding="5dp"
                        android:src="@mipmap/login_email_icon"
                        android:layout_width="30.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/ımageView2"
                        android:layout_marginLeft="15.0dp" />
                    <EditText
                        android:textSize="12dp"
                        android:paddingLeft="15dp"
                        android:background="@null"
                        android:textColor="#11122E"
                        android:hint="Soyadınız"
                        android:textColorHint="#9011122E"
                        android:layout_marginLeft="0.0dp"
                        android:layout_marginRight="0.0dp"
                        android:inputType="textEmailAddress"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/editText1" />
                </LinearLayout>
            </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/hesapolusturinputbg"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout2">
                            <ImageView
                                android:tint="#50ffffff"
                                android:padding="5dp"
                                android:src="@mipmap/mail_img1"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <EditText
                                android:textSize="12dp"
                                android:paddingLeft="15dp"
                                android:background="@null"
                                android:textColor="#11122E"
                                android:hint="Email Adresiniz"
                                android:textColorHint="#9011122E"
                                android:layout_marginLeft="0.0dp"
                                android:layout_marginRight="0.0dp"
                                android:inputType="textPassword"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/editText2" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/hesapolusturinputbg"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout2">
                            <ImageView
                                android:tint="#50ffffff"
                                android:padding="7dp"
                                android:src="@mipmap/login_lock_icon"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <EditText
                                android:textSize="12dp"
                                android:paddingLeft="15dp"
                                android:background="@null"
                                android:textColor="#11122E"
                                android:hint="Şifreniz"
                                android:textColorHint="#9011122E"
                                android:layout_marginLeft="0.0dp"
                                android:layout_marginRight="0.0dp"
                                android:inputType="textPassword"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/editText2" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:weightSum="100"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout4"
                            android:layout_marginTop="15.5dp">
                            <LinearLayout
                                android:background="@drawable/hesapolusturinputbg"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_weight="100"
                                android:layout_height="50.0dp"
                                android:id="@+id/linearLayout1">
                                <ImageView
                                    android:tint="#5011122E"
                                    android:padding="5dp"
                                    android:src="@mipmap/calender_img1"
                                    android:layout_width="30.5dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/ımageView2"
                                    android:layout_marginLeft="15.0dp" />
                                <TextView
                                    android:gravity="center_vertical"
                                    android:textSize="12dp"
                                    android:paddingLeft="15dp"
                                    android:background="@null"
                                    android:textColor="#11122E"
                                    android:hint="Dogum Tarihiniz"
                                    android:textColorHint="#9011122E"
                                    android:layout_marginLeft="0.0dp"
                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textEmailAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/editText1" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:weightSum="100"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout4"
                            android:layout_marginTop="15.5dp">
                            <LinearLayout
                                android:layout_marginRight="10dp"
                                android:background="@drawable/hesapolusturinputbg"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_weight="50"
                                android:layout_height="50.0dp"
                                android:id="@+id/linearLayout1">
                                <ImageView
                                    android:tint="#5011122E"
                                    android:padding="8dp"
                                    android:src="@mipmap/location_img1"
                                    android:layout_width="30.5dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/ımageView2"
                                    android:layout_marginLeft="15.0dp" />
                                <TextView
                                    android:gravity="center_vertical"
                                    android:textSize="12dp"
                                    android:paddingLeft="15dp"
                                    android:background="@null"
                                    android:textColor="#11122E"
                                    android:hint="Şehiriniz"
                                    android:textColorHint="#9011122E"
                                    android:layout_marginLeft="0.0dp"
                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textEmailAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/editText1" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/hesapolusturinputbg"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="0dp"
                                android:layout_weight="50"
                                android:layout_height="50.0dp"
                                android:id="@+id/linearLayout1">
                                <ImageView
                                    android:tint="#5011122E"
                                    android:padding="8dp"
                                    android:src="@mipmap/location_img1"
                                    android:layout_width="30.5dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/ımageView2"
                                    android:layout_marginLeft="15.0dp" />
                                <TextView
                                    android:gravity="center_vertical"
                                    android:textSize="12dp"
                                    android:paddingLeft="15dp"
                                    android:background="@null"
                                    android:textColor="#11122E"
                                    android:hint="İlçe"
                                    android:textColorHint="#9011122E"
                                    android:layout_marginLeft="0.0dp"
                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textEmailAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/editText1" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="15dp"
                         android:layout_gravity="center_horizontal"
                         android:layout_marginTop="50dp"
                         android:orientation="horizontal"
                         android:minWidth="25px"
                         android:minHeight="25px"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:id="@+id/linearLayout15" >
                         <Button
                             android:textSize="10dp"
                             android:textColor="#fff"
                             android:background="@drawable/profiliguncellebuttonbg"
                             android:text="PROFİLİNİ GÜNCELLE"
                             android:layout_width="match_parent"
                             android:layout_height="45dp"
                             android:id="@+id/button2" />
                    </LinearLayout>
                </LinearLayout>
        </ScrollView>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
