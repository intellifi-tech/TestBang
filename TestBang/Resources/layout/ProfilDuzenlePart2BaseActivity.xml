                                android:inputType="textPassword"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/editText5" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:weightSum="100"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout4"
                            android:layout_marginTop="15.5dp">
                            <LinearLayout
                                android:background="@drawable/hesapolusturinputbg"
                                android:orientation="horizontal"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="50.0dp"
                                android:id="@+id/linearLayout1">
                                <ImageView
                                    android:tint="#5011122E"
                                    android:padding="5dp"
                                    android:src="@mipmap/calender_img1"
                                    android:layout_width="30.5dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/ımageView2"
                                    android:layout_marginLeft="15.0dp" />
                                <TextView
                                    android:fontFamily="@font/monserratfont"
                                    android:gravity="center_vertical"
                                    android:textSize="16dp"
                                    android:paddingLeft="15dp"
                                    android:background="@null"
                                    android:textColor="#11122E"
                                    android:hint="Doğum Tarihiniz"
                                    android:textColorHint="#9011122E"
                                    android:layout_marginLeft="0.0dp"
                                    android:layout_marginRight="0.0dp"
                                    android:inputType="textEmailAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/dogumtxt" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:layout_marginTop="15.5dp"
                            android:background="@drawable/hesapolusturinputbg"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout1">
                            <ImageView
                                android:tint="#5011122E"
                                android:padding="8dp"
                                android:src="@mipmap/gendericonn"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/cinsiyetspin" />
                        </LinearLayout>
                             <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="15.5dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:tint="#5011122E"
                    android:padding="8dp"
                    android:src="@mipmap/location_img1"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/sehirspin" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="15.5dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:tint="#5011122E"
                    android:padding="8dp"
                    android:src="@mipmap/location_img1"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/ilcespin" />
            </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:layout_marginTop="15.5dp"
                            android:background="@drawable/hesapolusturinputbg"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout1">
                            <ImageView
                                android:tint="#5011122E"
                                android:padding="8dp"
                                android:src="@mipmap/konuiconimg1"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/alanspin" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:layout_marginTop="15.5dp"
                            android:background="@drawable/hesapolusturinputbg"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout1">
                            <ImageView
                                android:tint="#5011122E"
                                android:padding="8dp"
                                android:src="@mipmap/okul_img1"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/okulspin" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="15dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="50dp"
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout15">
                        <Button
                            android:fontFamily="@font/monserratfontbold"
                            android:textSize="10dp"
                            android:textColor="#fff"
                            android:background="@drawable/profiliguncellebuttonbg"
