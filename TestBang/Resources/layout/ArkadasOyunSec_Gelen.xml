<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#11122E"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img3"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout
            android:orientation="vertical"
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/cardView1">
                <LinearLayout
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout1">
                    <TextView
                        android:fontFamily="@font/monserratfontbold"
                        android:layout_marginTop="25dp"
                        android:layout_marginBottom="17.0dp"
                        android:textSize="30dp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textColor="#000"
                        android:text="MEYDAN OKUMA ALDIN!"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/denemeaditext"
                        android:layout_marginRight="20dp"
                        android:layout_marginLeft="20dp" />
                    <TextView
                        android:layout_marginBottom="20.0dp"
                        android:fontFamily="@font/monserratfont"
                        android:gravity="center"
                        android:text="Rakibin sana medan okudu!"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3" />
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout5">
                        <LinearLayout
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout6">
                            <refractored.controls.CircleImageView
                                android:layout_centerInParent="true"
                                android:id="@+id/profile_image1"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:src="@mipmap/animuserimg"
                                app:civ_border_width="0dp"
                                app:civ_border_color="#fff"/>
                            <TextView
                                android:ellipsize="marquee"
                                android:textColor="#FF6064"
                                android:fontFamily="@font/monserratfontbold"
                                android:gravity="center"
                                android:text="Lorem Ipsum"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/textView4" />
                        </LinearLayout>
                        <ImageView
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:tint="#1EB04B"
                            android:src="@mipmap/oyun3"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView1" />
                        <LinearLayout
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout6">
                            <refractored.controls.CircleImageView
                                android:layout_centerInParent="true"
                                android:id="@+id/profile_image2"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:src="@mipmap/animuserimg"
                                app:civ_border_width="0dp"
                                app:civ_border_color="#fff"/>
                            <TextView
                                android:ellipsize="marquee"
                                android:textColor="#FF6064"
                                android:fontFamily="@font/monserratfontbold"
                                android:gravity="center"
                                android:text="Lorem Ipsum"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/textView5" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout7" >
                        <TextView
                            android:textSize="35dp"
                            android:fontFamily="@font/monserratfontbold"
                            android:gravity="center"
                            android:text="01:00"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:layout_marginBottom="17.0dp"
                            android:layout_marginTop="46.5dp" />
                        <LinearLayout
                            android:weightSum="100"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout8" >
                         <Button
                             android:layout_gravity="center_vertical"
                             android:fontFamily="@font/monserratfontbold"
                             android:textSize="10dp"
                             android:textColor="#fff"
                             android:background="@drawable/profiliguncellebuttonbg"
                             android:text="KABUL ET"
                             android:layout_width="0dp"
                             android:layout_weight="50"
                             android:layout_height="45dp"
                             android:layout_marginRight="5dp"
                             android:id="@+id/button3" />
                         <Button
                             android:layout_gravity="center_vertical"
                             android:fontFamily="@font/monserratfontbold"
                             android:layout_marginLeft="5dp"
                             android:textSize="10dp"
                             android:textColor="#fff"
                             android:background="@drawable/ayarlarbuttonbg"
                             android:text="REDDET"
                             android:layout_width="0dp"
                             android:layout_weight="50"
                             android:layout_height="45dp"
                             android:id="@+id/button2" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
