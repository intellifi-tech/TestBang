<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <android.support.v7.widget.CardView
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cardView1" >
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout1" >
                    <RelativeLayout
                        android:paddingTop="10dp"
                        android:paddingLeft="10dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/relativeLayout1">
                        <RelativeLayout
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/relativeLayout2">
                            <LinearLayout
                                android:layout_toLeftOf="@+id/relativeLayout3"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout2">
                                <TextView
                                    android:textColor="#11122e"
                                    android:text="Selam,"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView1" />
                                <TextView
                                    android:textSize="20dp"
                                    android:textStyle="bold"
                                    android:textColor="#F05070"
                                    android:text="MERVE TUĞAL"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView1" />
                               <TextView
                                    android:layout_marginTop="5dp"
                                    android:textSize="11dp"
                                    android:textColor="#11122e"
                                    android:text="TURNASUYU, ORDU"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView1" />
                            </LinearLayout>
                            <RelativeLayout
                                android:layout_alignParentRight="true"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/relativeLayout3" >
                                <refractored.controls.CircleImageView
                                    android:background="@mipmap/profilebgimg1"
                                      android:layout_centerInParent="true"
                                      android:id="@+id/profile_image"
                                      android:layout_width="match_parent"
                                      android:layout_height="match_parent"
                                      android:src="@mipmap/ornekfoto"
                                      app:civ_border_width="16dp"
                                      app:civ_border_color="@android:color/transparent"/>
                            </RelativeLayout>
                        </RelativeLayout>
                    </RelativeLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout12" >
                        <LinearLayout
                            android:background="@drawable/profilemenubg"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/linearLayout13">
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_1"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="13dp"
                                android:src="@mipmap/profile_icon_4"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="11dp"
                                android:src="@mipmap/profile_icon_2"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_3"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_5"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_6"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_7"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                            <ImageView
                                android:padding="10dp"
                                android:src="@mipmap/profile_icon_8"
                                android:layout_width="60dp"
                                android:layout_height="50dp"
                                android:id="@+id/ımageView1" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="10dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout14" >
                            <TextView
                                android:textColorHighlight="#1EB04B"
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="PROFİLİM"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/profilim" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="ÜYELİK / PAKET"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/uyelik" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="DENEME SINAVI TRANSKRİPTLERİ"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="SINAV PROGRAMI"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="ARKADAŞINI DAVET ET"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="SIKÇA SORULAN SORULAR"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="TESTBANG HAKKINDA"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                            <TextView
                                android:textStyle="bold"
                                android:textSize="13dp"
                                android:textColorHint="#201632"
                                android:gravity="center_vertical"
                                android:hint="İLETİŞİM"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:id="@+id/textView2" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <LinearLayout
                android:layout_marginBottom="15dp"
                 android:layout_gravity="center_horizontal"
                 android:layout_marginTop="15dp"
                 android:orientation="horizontal"
                 android:minWidth="25px"
                 android:minHeight="25px"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:id="@+id/linearLayout15" >
                 <Button
                     android:textSize="10dp"
                     android:textColor="#fff"
                     android:background="@drawable/oturumkapatbuttonbg"
                     android:text="OTURUMU KAPAT"
                     android:layout_width="150dp"
                     android:layout_height="45dp"
                     android:layout_marginRight="5dp"
                     android:id="@+id/button3" />
                 <Button
                     android:layout_marginLeft="5dp"
                     android:textSize="10dp"
                     android:textColor="#fff"
                     android:background="@drawable/ayarlarbuttonbg"
                     android:text="AYARLAR"
                     android:layout_width="150dp"
                     android:layout_height="45dp"
                     android:id="@+id/button2" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>