
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.widget.NestedScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <android.support.v7.widget.CardView
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cardView1" >
               <LinearLayout
                   android:layout_margin="10dp"
                   android:orientation="vertical"
                   android:minWidth="25px"
                   android:minHeight="25px"
                   android:layout_width="match_parent"
                   android:layout_height="match_parent"
                   android:id="@+id/linearLayout1">
               <TextView
                   android:gravity="center"
                   android:textColor="#11122E"
                   android:text="COĞRAFYA"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:id="@+id/derstext" />
               <TextView
                   android:layout_marginTop="5dp"
                   android:textSize="20dp"
                   android:textStyle="bold"
                   android:gravity="center"
                   android:textColor="#11122E"
                   android:text="SORU 1/49"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:id="@+id/curentsorunumber" />
                <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxHeight="250dp"
                        android:id="@+id/soruimage" />
                <TextView
                   android:layout_marginTop="10dp"
                   android:textSize="14dp"
                   android:textStyle="bold"
                   android:gravity="left"
                   android:textColor="#11122E"
                   android:text="10 Kuzey paralelinden aynı meridyen üzerinde kuzey yönünde 1887 km gidildiğinde, aşağıdaki parallerden hangisine ulaşılır?"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:id="@+id/sorutext" />
                   
               </LinearLayout>
            </android.support.v7.widget.CardView>
            <LinearLayout
                android:layout_marginTop="25dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout12" >
                 <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/relative_a" >
                    <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout13" >
                    <Button
                        android:textColor="#11122E"
                        android:textSize="20dp"
                        android:background="@android:color/transparent"
                        android:textStyle="bold"
                        android:text="A)"
                        android:layout_width="45.0dp"
                        android:layout_height="45.0dp"
                        android:id="@+id/button_a" />
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#11122E"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="7 Kuzey Paraleli "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/answer_a" />
                  </LinearLayout>
                    <ImageView
                        android:layout_marginLeft="45dp"
                        android:layout_below="@+id/linearLayout13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxHeight="125dp"
                        android:id="@+id/answer_image_a" />
                </RelativeLayout>
                 <View
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="#919191"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:id="@+id/view1" />
                 <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/relative_b" >
                    <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout13" >
                    <Button
                        android:textColor="#11122E"
                        android:textSize="20dp"
                        android:background="@android:color/transparent"
                        android:textStyle="bold"
                        android:text="B)"
                        android:layout_width="45.0dp"
                        android:layout_height="45.0dp"
                        android:id="@+id/button_b" />
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#11122E"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="7 Kuzey Paraleli "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/answer_b" />
                  </LinearLayout>
                    <ImageView
                        android:layout_marginLeft="45dp"
                        android:layout_below="@+id/linearLayout13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxHeight="125dp"
                        android:id="@+id/answer_image_b" />
                </RelativeLayout>
                 <View
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="#919191"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:id="@+id/view1" />
                 <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/relative_c" >
                    <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout13" >
                    <Button
                        android:textColor="#11122E"
                        android:textSize="20dp"
                        android:background="@android:color/transparent"
                        android:textStyle="bold"
                        android:text="C)"
                        android:layout_width="45.0dp"
                        android:layout_height="45.0dp"
                        android:id="@+id/button_c" />
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#11122E"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="7 Kuzey Paraleli "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/answer_c" />
                  </LinearLayout>
                    <ImageView
                        android:layout_marginLeft="45dp"
                        android:layout_below="@+id/linearLayout13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxHeight="125dp"
                        android:id="@+id/answer_image_c" />
                </RelativeLayout>
                 <View
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="#919191"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:id="@+id/view1" />
                 <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/relative_d" >
                    <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout13" >
                    <Button
                        android:textColor="#11122E"
                        android:textSize="20dp"
                        android:background="@android:color/transparent"
                        android:textStyle="bold"
                        android:text="D)"
                        android:layout_width="45.0dp"
                        android:layout_height="45.0dp"
                        android:id="@+id/button_d" />
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#11122E"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="7 Kuzey Paraleli "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/answer_d" />
                  </LinearLayout>
                    <ImageView
                        android:layout_marginLeft="45dp"
                        android:layout_below="@+id/linearLayout13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxHeight="125dp"
                        android:id="@+id/answer_image_d" />
                </RelativeLayout>
                 <View
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="#919191"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:id="@+id/view1" />
                 <RelativeLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/relative_e" >
                    <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout13" >
                    <Button
                        android:textColor="#11122E"
                        android:textSize="20dp"
                        android:background="@android:color/transparent"
                        android:textStyle="bold"
                        android:text="E)"
                        android:layout_width="45.0dp"
                        android:layout_height="45.0dp"
                        android:id="@+id/button_e" />
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#11122E"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="7 Kuzey Paraleli "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/answer_e" />
                  </LinearLayout>
                    <ImageView
                        android:layout_marginLeft="45dp"
                        android:layout_below="@+id/linearLayout13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxHeight="125dp"
                        android:id="@+id/answer_image_e" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</RelativeLayout>