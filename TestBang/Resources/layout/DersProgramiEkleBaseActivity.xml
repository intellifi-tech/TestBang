<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F05070"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img2"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout 
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <android.support.v7.widget.CardView
                android:visibility="visible"
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cardView1" >
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout1" >
                    <TextView
                        android:layout_marginTop="15dp"
                        android:textColor="#1EB14C"
                        android:gravity="center_horizontal"
                        android:textStyle="bold"
                        android:text="SINAV PROGRAMI"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2" />
                     <TextView
                        android:layout_marginTop="5dp"
                        android:textColor="#11122E"
                        android:gravity="center_horizontal"
                        android:text="Merve Tuğral"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3" />
                     <TextView
                        android:layout_marginRight="20dp"
                        android:layout_marginLeft="20dp"
                        android:textSize="12dp"
                        android:layout_marginTop="5dp"
                        android:textColor="#11122E"
                        android:gravity="center_horizontal"
                        android:text="İleri bir tarihe deneme veya test programı girebilir, sınav günü size hatırlatma ve kendini takipte tutabilirsiniz."
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView33" />
                        <RelativeLayout
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="25dp"
                        android:background="@drawable/programekleinput"
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="50.0dp"
                        android:id="@+id/linearLayout2">
                        <ImageView
                            android:tint="#F05070"
                            android:padding="5dp"
                            android:src="@mipmap/konuiconimg1"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                        <TextView
                            android:gravity="center_vertical"
                            android:textColor="#11122E"
                            android:layout_marginLeft="5dp"
                            android:textSize="13dp"
                            android:hint="Test Saati"
                            android:background="@android:color/transparent"
                            android:layout_toRightOf="@+id/ımageView2"
                            android:inputType="textPersonName"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/testsaati" />
                    </RelativeLayout>
                        <RelativeLayout
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="25dp"
                        android:background="@drawable/programekleinput"
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="50.0dp"
                        android:id="@+id/linearLayout22">
                        <ImageView
                            android:tint="#F05070"
                            android:padding="5dp"
                            android:src="@mipmap/konuiconimg1"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                        <EditText
                            android:textColor="#11122E"
                            android:layout_marginLeft="5dp"
                            android:textSize="13dp"
                            android:hint="Test Adı"
                            android:background="@android:color/transparent"
                            android:layout_toRightOf="@+id/ımageView2"
                            android:inputType="textPersonName"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/testadittext" />
                    </RelativeLayout>
                        <RelativeLayout
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/programekleinput"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout222">
                            <ImageView
                                android:tint="#F05070"
                                android:padding="5dp"
                                android:src="@mipmap/konuiconimg1"
                                android:layout_width="30.5dp"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView2"
                                android:layout_marginLeft="15.0dp" />
                            <EditText
                                android:textColor="#11122E"
                                android:layout_marginLeft="5dp"
                                android:textSize="13dp"
                                android:hint="Test Açıklaması"
                                android:background="@android:color/transparent"
                                android:layout_toRightOf="@+id/ımageView2"
                                android:inputType="textPersonName"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/testaciklamasitext" />
                        </RelativeLayout>
                        <LinearLayout
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/kirmiziborder"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout1">
                        <ImageView
                            android:tint="#5011122E"
                            android:padding="8dp"
                            android:src="@mipmap/derssecimg1"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/spinner1" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/kirmiziborder"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout2222">
                        <ImageView
                            android:tint="#5011122E"
                            android:padding="8dp"
                            android:src="@mipmap/konusecimg"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/spinner2" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/girisbuton"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout3">
                        <ImageView
                            android:tint="#5011122E"
                            android:padding="8dp"
                            android:src="@mipmap/sorusayisisecimg"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/spinner3" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="15dp"
                            android:layout_marginBottom="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:background="@drawable/girisbuton"
                            android:orientation="horizontal"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="50.0dp"
                            android:id="@+id/linearLayout4">
                            <ImageView
                            android:tint="#5011122E"
                            android:padding="8dp"
                            android:src="@mipmap/suresecimg"
                            android:layout_width="30.5dp"
                            android:layout_height="match_parent"
                            android:id="@+id/ımageView2"
                            android:layout_marginLeft="15.0dp" />
                            <Spinner
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/spinner4" />
                        </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
              <Button
                     android:layout_marginTop="15dp"
                     android:layout_marginBottom="15dp"
                     android:layout_marginRight="30dp"
                     android:layout_marginLeft="30dp"
                     android:textSize="10dp"
                     android:textColor="#fff"
                     android:background="@drawable/profiliguncellebuttonbg"
                     android:text="DERS PROGRAMI EKLE"
                     android:layout_width="match_parent"
                     android:layout_height="45dp"
                     android:id="@+id/button2" />
           </LinearLayout>
        </ScrollView>
      </LinearLayout>
    </RelativeLayout>
</LinearLayout>
