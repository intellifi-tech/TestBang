<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F05070"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img2"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout 
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ScrollView
        android:scrollbars="none"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout11">
            <androidx.cardview.widget.CardView
                android:visibility="visible"
                app:cardElevation="10dp"
                app:cardCornerRadius="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cardView1" >
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout1">
                            <TextView
                                android:layout_marginTop="15dp"
                                android:textColor="#1EB14C"
                                android:gravity="center_horizontal"
                                android:textStyle="bold"
                                android:text="SINAV PROGRAMI"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/textView2" />
                            <TextView
                                android:layout_marginTop="5dp"
                                android:textColor="#11122E"
                                android:gravity="center_horizontal"
                                android:text="Merve Tuğral"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@id/adsoyadtext" />
                            <TextView
                                android:layout_marginRight="20dp"
                                android:layout_marginLeft="20dp"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp"
                                android:textColor="#11122E"
                                android:gravity="center_horizontal"
                                android:text="İleri bir tarihe deneme veya test programı girebilir, sınav günü size hatırlatma ve kendini takipte tutabilirsiniz."
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/textView3" />
                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout12">
                                <RelativeLayout
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/relativeLayout5" >
                                    <ImageButton
                                        android:background="?android:attr/selectableItemBackground"
                                        android:src="@drawable/chevrons_left"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:id="@+id/ımageButton1" />
                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:minWidth="25px"
                                        android:minHeight="25px"
                                        android:layout_width="match_parent"
                                        android:layout_height="40dp"
                                        android:layout_toRightOf="@+id/ımageButton1"
                                        android:layout_toLeftOf="@+id/ımageButton2"
                                        android:id="@+id/linearLayout13" >
                                        <TextView
                                            android:textStyle="bold"
                                            android:textSize="15dp"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:text="NİSAN"
                                            android:textAppearance="?android:attr/textAppearanceLarge"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/takvimayaditext" />
                                        <TextView
                                            android:textSize="15dp"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:text="2020"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/takvimyiltext" />
                                    </LinearLayout>
                                    <ImageButton
                                        android:background="?android:attr/selectableItemBackground"
                                        android:layout_alignParentRight="true"
                                        android:src="@drawable/chevrons_right"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:id="@+id/ımageButton2" />
                                </RelativeLayout>
                                <LinearLayout
                                    android:weightSum="98"
                                    android:orientation="horizontal"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:id="@+id/linearLayout14" >
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="P.TESİ"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="SALI"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="ÇARŞ."
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="PERŞ."
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="CUMA"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="C.TESİ"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                    <TextView
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:textStyle="bold"
                                        android:textColor="#000"
                                        android:text="PAZAR"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:layout_weight="14"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/textView6" />
                                </LinearLayout>
                                <GridView
                                    android:numColumns="7"
                                    android:minWidth="25px"
                                    android:minHeight="25px"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:id="@+id/gridView1" />
                            </LinearLayout>
                        </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.recyclerview.widget.RecyclerView
                 android:layout_marginTop="15dp"
                 android:minWidth="25px"
                 android:minHeight="25px"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:id="@+id/recyclerView1" />
            <Button
                     android:layout_marginTop="15dp"
                     android:layout_marginBottom="15dp"
                     android:layout_marginRight="30dp"
                     android:layout_marginLeft="30dp"
                     android:textSize="10dp"
                     android:textColor="#fff"
                     android:background="@drawable/profiliguncellebuttonbg"
                     android:text="DERS PROGRAMI EKLE"
                     android:layout_width="match_parent"
                     android:layout_height="45dp"
                     android:id="@+id/button2" />
           </LinearLayout>
        </ScrollView>
      </LinearLayout>
    </RelativeLayout>
</LinearLayout>
