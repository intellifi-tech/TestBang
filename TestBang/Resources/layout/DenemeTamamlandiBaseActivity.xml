<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#10102A"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img3"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="0.0dp">
        <RelativeLayout
            android:background="#fff"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="60dp" />
        <LinearLayout 
            android:orientation="vertical"
            android:background="@android:color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <androidx.core.widget.NestedScrollView
            android:scrollbars="none"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView1">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout11">
                    <androidx.cardview.widget.CardView
                        app:cardElevation="10dp"
                        app:cardCornerRadius="20dp"
                        android:layout_marginRight="30dp"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="214.0dp"
                        android:id="@+id/cardView1">
                        <LinearLayout
                            android:layout_margin="10dp"
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout1">
                            <ImageView
                                android:src="@mipmap/oyunukazandinimg"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/ımageView1" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <TextView
                        android:layout_marginTop="46.0dp"
                        android:layout_marginBottom="110.0dp"
                        android:textColor="#10102A"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="DENEME SINAVI TAMAMLANDI!"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="25dp"
                        android:id="@+id/textView1"
                        android:layout_marginLeft="50dp"
                        android:layout_marginRight="50.5dp" />
                <Button
                        android:layout_marginRight="30dp"
                        android:layout_marginLeft="30dp"
                         android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp"
                        android:textSize="10dp"
                        android:textColor="#fff"
                        android:background="@drawable/ayarlarbuttonbg"
                        android:text="TAMAM"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:id="@+id/button3" />
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
      </LinearLayout>
    </RelativeLayout>
</LinearLayout>
