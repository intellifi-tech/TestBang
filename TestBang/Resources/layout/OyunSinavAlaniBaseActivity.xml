        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="65.5dp"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="17.5dp">
        <ImageView
            android:scaleType="centerInside"
            android:src="@mipmap/testbang_logo_img3"
            android:layout_width="163.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ımageView1"
            android:layout_marginLeft="25.0dp" />
        <LinearLayout
                android:clickable="true"
                android:layout_alignParentRight="true"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/bildirimhaznebutton"
                android:layout_marginRight="25dp">
                <ImageButton
                    android:padding="8dp"
                    android:scaleType="centerInside"
                    android:tint="#ffffff"
                    android:background="?android:attr/selectableItemBackground"
                    android:src="@drawable/pencil"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageButton1" />
            </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
