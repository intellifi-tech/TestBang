                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="0dp"
                        android:layout_weight="10"
                        android:layout_height="6dp"
                        android:id="@+id/linearLayoutben1" />
