<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#fff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout5">
            <ImageView
                android:scaleType="centerInside"
                android:src="@mipmap/testbang_logo_img1"
                android:layout_width="match_parent"
                android:layout_height="71.0dp"
                android:minWidth="25px"
                android:minHeight="25px"
                android:id="@+id/ımageView1"
                android:layout_marginTop="20.0dp" />
            <TextView
                android:fontFamily="@font/monserratfont"
                android:textSize="18dp"
                android:textStyle="bold"
                android:textColor="#11122E"
                android:gravity="center_horizontal"
                android:text="TestBang'e"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView1"
                android:layout_marginTop="38.5dp" />
            <TextView
                android:fontFamily="@font/monserratfontbold"
                android:textSize="24dp"
                android:textStyle="bold"
                android:textColor="#F05070"
                android:gravity="center_horizontal"
                android:text="ÜYE OLUN"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView2" />
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:weightSum="100"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout4"
                android:layout_marginTop="15.5dp">
                <LinearLayout
                    android:layout_marginRight="10dp"
                    android:background="@drawable/hesapolusturinputbg"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="0dp"
                    android:layout_weight="50"
                    android:layout_height="50.0dp"
                    android:id="@+id/linearLayout1">
                    <ImageView
                        android:tint="#5011122E"
                        android:padding="5dp"
                        android:src="@mipmap/login_email_icon"
                        android:layout_width="30.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/ımageView2"
                        android:layout_marginLeft="15.0dp" />
                    <EditText
                        android:fontFamily="@font/monserratfont"
                        android:textCursorDrawable="@null"
                        android:textSize="16dp"
                        android:paddingLeft="15dp"
                        android:background="@null"
                        android:textColor="#11122E"
                        android:hint="Adınız"
                        android:textColorHint="#9011122E"
                        android:layout_marginLeft="0.0dp"
                        android:layout_marginRight="0.0dp"
                        android:inputType="textPersonName"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/editText1" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/hesapolusturinputbg"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="0dp"
                    android:layout_weight="50"
                    android:layout_height="50.0dp"
                    android:id="@+id/linearLayout1">
                    <ImageView
                        android:tint="#5011122E"
                        android:padding="5dp"
                        android:src="@mipmap/login_email_icon"
                        android:layout_width="30.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/ımageView2"
                        android:layout_marginLeft="15.0dp" />
                    <EditText
                        android:fontFamily="@font/monserratfont"
                        android:textCursorDrawable="@null"
                        android:textSize="16dp"
                        android:paddingLeft="15dp"
                        android:background="@null"
                        android:textColor="#11122E"
                        android:hint="Soyadınız"
                        android:textColorHint="#9011122E"
                        android:layout_marginLeft="0.0dp"
                        android:layout_marginRight="0.0dp"
                        android:inputType="textPersonName"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/editText2" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout2">
                <ImageView
                    android:tint="#50ffffff"
                    android:padding="5dp"
                    android:src="@mipmap/mail_img1"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <EditText
                    android:fontFamily="@font/monserratfont"
                    android:textCursorDrawable="@null"
                    android:textSize="16dp"
                    android:paddingLeft="15dp"
                    android:background="@null"
                    android:textColor="#11122E"
                    android:hint="Email Adresiniz"
                    android:textColorHint="#9011122E"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="0.0dp"
                    android:inputType="textEmailAddress"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText3" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout2">
                <ImageView
                    android:tint="#50ffffff"
                    android:padding="7dp"
                    android:src="@mipmap/login_lock_icon"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <EditText
                    android:fontFamily="@font/monserratfont"
                    android:textCursorDrawable="@null"
                    android:textSize="16dp"
                    android:paddingLeft="15dp"
                    android:background="@null"
                    android:textColor="#11122E"
                    android:hint="Şifreniz"
                    android:textColorHint="#9011122E"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="0.0dp"
                    android:inputType="textPassword"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText4" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout2">
                <ImageView
                    android:tint="#50ffffff"
                    android:padding="7dp"
                    android:src="@mipmap/login_lock_icon"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <EditText
                    android:fontFamily="@font/monserratfont"
                    android:textCursorDrawable="@null"
                    android:textSize="16dp"
                    android:paddingLeft="15dp"
                    android:background="@null"
                    android:textColor="#11122E"
                    android:hint="Şifrenizi Tekrar Girin"
                    android:textColorHint="#9011122E"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="0.0dp"
                    android:inputType="textPassword"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText5" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:weightSum="100"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout4"
                android:layout_marginTop="15.5dp">
                <LinearLayout
                    android:background="@drawable/hesapolusturinputbg"
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="50.0dp"
                    android:id="@+id/linearLayout1">
                    <ImageView
                        android:tint="#5011122E"
                        android:padding="5dp"
                        android:src="@mipmap/calender_img1"
                        android:layout_width="30.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/ımageView2"
                        android:layout_marginLeft="15.0dp" />
                    <TextView
                        android:fontFamily="@font/monserratfont"
                        android:gravity="center_vertical"
                        android:textSize="16dp"
                        android:paddingLeft="15dp"
                        android:background="@null"
                        android:textColor="#11122E"
                        android:hint="Doğum Tarihiniz"
                        android:textColorHint="#9011122E"
                        android:layout_marginLeft="0.0dp"
                        android:layout_marginRight="0.0dp"
                        android:inputType="textEmailAddress"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/dogumtxt" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="15.5dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:tint="#5011122E"
                    android:padding="8dp"
                    android:src="@mipmap/gendericonn"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/cinsiyetspin" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="15.5dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:tint="#5011122E"
                    android:padding="8dp"
                    android:src="@mipmap/location_img1"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/sehirspin" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="15.5dp"
                android:background="@drawable/hesapolusturinputbg"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:tint="#5011122E"
                    android:padding="8dp"
                    android:src="@mipmap/location_img1"
                    android:layout_width="30.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/ımageView2"
                    android:layout_marginLeft="15.0dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/ilcespin" />
            </LinearLayout>
            <Button
                android:fontFamily="@font/monserratfontbold"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#fff"
                android:background="@drawable/girisbuttonbg"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:text="HEMEN ÜYE OL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/button1" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
